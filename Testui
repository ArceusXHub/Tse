if World3 then
local Dropdown = Tabs.Items:AddDropdown("Bone Farm Mode", {
    Title = "Bone Farm Mode",
    Values = {"No Quest", "Accept Quest", "Mastery Fruit"},
    Multi = false,
    Default = 1,
})

